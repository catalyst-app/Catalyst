<If "%{REMOTE_ADDR} !='::1'">
	AuthType Basic
	AuthName "Approved testers only"
	AuthUserFile /home/catalyst/.htpasswd
	Require valid-user

	RewriteEngine On 
	RewriteCond %{HTTP_HOST} ^beta\.catalystapp\.co [NC]
	RewriteCond %{SERVER_PORT} 80 
	RewriteRule ^(.*)$ https://beta.catalystapp.co/$1 [R=301,L]
	RewriteBase /
	RewriteRule ^index\.php$ - [L]
	RewriteCond %{REQUEST_FILENAME} !-f
	RewriteCond %{REQUEST_FILENAME} !-d
	RewriteRule . /index.php [L]
</If>

<IfModule mod_speling.c>
	CheckCaseOnly on
	CheckSpelling on
</IfModule>

<IfModule mod_negotiation.c>
	Options -MultiViews
</IfModule>

<IfModule mod_php5.c>
	php_value upload_max_filesize 10M 
</IfModule>
<IfModule mod_php7.c>
	php_value upload_max_filesize 10M 
</IfModule>

Options -Indexes
